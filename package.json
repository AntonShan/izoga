{
    "version": "1.0.0",
    "name": "izoga",
    "packageManager": "yarn@3.6.1",
    "scripts": {
        "dev": "ts-node src/main.ts",
        "test": "jest",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "generate-blob": "node --experimental-sea-config sea-config.json",
