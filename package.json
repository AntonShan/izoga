        "unzipper": "0.12.3",
        "yargs": "17.7.2",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.0",
        "@eslint/js": "9.22.0",
        "@total-typescript/ts-reset": "0.6.1",
        "@types/bent": "7.3.8",
        "@types/jest": "29.5.14",
        "@types/node": "22.13.10",
        "@types/unzipper": "^0.10.11",
        "@types/yargs": "17.0.33",
        "@vercel/ncc": "0.38.3",
        "eslint": "9.22.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "globals": "16.0.0",
        "jest": "29.7.0",
        "prettier": "3.5.3",
        "ts-jest": "29.2.6",
        "ts-node": "10.9.2",
        "tsx": "4.19.3",
        "typescript": "5.8.2",
        "typescript-eslint": "8.27.0"
    }
